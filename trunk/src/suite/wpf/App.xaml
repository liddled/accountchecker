<Application x:Class="DL.AccountChecker.Suite.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Skins\Generic.xaml"/>
                <ResourceDictionary Source="Skins\Default\Resources.xaml"/>
                <ResourceDictionary Source="Skins\Default\Accounts.xaml"/>
                <ResourceDictionary Source="Skins\Default\Categories.xaml"/>
                <ResourceDictionary Source="Skins\Default\Reports.xaml"/>
                <ResourceDictionary Source="Skins\Default\Transactions.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
